databaseChangeLog:
  - changeSet:
      id: 1
      author: Furkan
      preConditions:
        - onFail: MARK_RAN
          not:
            tableExists:
              tableName: test_table
      changes:
        - createTable:
            tableName: test_table
            columns:
              - column:
                  name: id
                  type: bigint
                  autoIncrement: true
                  constraints:
                    primaryKey: true
                    nullable: false
              - column:
                  name: first_name
                  type: varchar(250)
                  constraints:
                    nullable: false
  - changeSet:
      id: 2
      author: Furkan
      changes:
        - insert:
            tableName: test_table
            columns:
              - column:
                  name: first_name
                  value: "Furkan"